//{ Driver Code Starts
#include<bits/stdc++.h>
using namespace std;


// } Driver Code Ends

class Solution
{
    public:
    //Function to check if brackets are balanced or not.
    bool ispar(string x)
    {
        // Your code here
        unordered_map<char,char>nm;
        nm[')']='(';
        nm[']']='[';
        nm['}']='{';
        
        stack<char>st;
        for(auto c:x){
            if(c=='(' or c=='[' or c=='{')st.push(c);
            else if(st.empty())return false;
        else{
            if(st.top()!=nm[c])return false;
            st.pop();
        }
    }
    return st.empty();
}
};

//{ Driver Code Starts.

int main()
{
   int t;
   string a;
   cin>>t;
   while(t--)
   {
       cin>>a;
       Solution obj;
       if(obj.ispar(a))
        cout<<"balanced"<<endl;
       else
        cout<<"not balanced"<<endl;
   }
}
// } Driver Code Ends
